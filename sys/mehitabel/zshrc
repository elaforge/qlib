# stick local stuff in stash to avoid going over nfs
s=/usr/local/google # easy to type interactive
stash=$s # more descriptive
src=$stash/src
dl=$stash/dl

# interactive only shell vars and path elements go here
export PLAN9=$stash/plan9
path=($path $stash/plan9/bin)

function g5 {
  (cwd && /google/data/ro/projects/shelltoys/g5.sar "$@")
}

function buildifier {
  (cwd && command buildifier "$@")
}

alias avi='vim -u $HOME/.vimrc --servername vim'

alias g4d=/usr/lib/piper-client/internal/g4d_command_impl.sh
alias placer=/google/data/ro/projects/placer/placer

