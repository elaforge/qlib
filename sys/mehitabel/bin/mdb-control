#!/bin/sh

export MDB_REMOTE_HOST=`hostname`
export MDB_WORKER_HOST=`hostname`
export MDB_APP_SERVER_HOST=`hostname`
export MDB_REPLICA_HOST=`hostname`
export MDB_SQL_APP_SERVER_HOST=`hostname`
export MDB_MASTER_HOST=`hostname`
export MDB_DB_NAME=mdb
export MDB_DB_USER=mdb_test
export MDB_DB_PASSWD=mdbpw
export MDB_DB_PORT=3306
export MDB_DUMP_LOCK_DIR=/export/hda3/datadumps
export MDB_TEST_ROLE=mdb_master
export MDB_MASTER_HOST=`hostname`
export MDB_TEST_MASTER_HOST=`hostname`
export MDB_OPERATION_LOG=/export/hda3/tmp/mdb_operations
export MDB_OPLOG_MUTEX=/export/hda3/tmp/.mdb_operations_lock
export MDB_BIN=$stash/google3/linux-dbg/production/mdb/mdb_control.par
exec $MDB_BIN "$@"
