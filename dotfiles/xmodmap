keycode 66 = Control_L
keycode 50 = Shift_L
keycode 62 = Shift_R
keycode 37 = Control_L

keycode 107 = Delete

! left windows key
keycode 115 = Hyper_L

! right windows key
keycode 116 = Hyper_R

clear shift
clear control
clear lock
add shift = Shift_L Shift_R
add mod5 = Hyper_L Hyper_R
add control = Control_L Control_R

! make sure shift+space still inserts a space, since it's easy to type
! a shifted character followed by a shifted space
keysym space = space space space space
